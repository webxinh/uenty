{
    "name": "aabcsoft/aabc2-app-advanced",
    "description": "aabc 2 Advanced Project Template",
    "keywords": ["aabc2", "framework", "advanced", "project template"],
    "homepage": "http://www.aabcframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/aabcsoft/aabc2/issues?state=open",
        "forum": "http://www.aabcframework.com/forum/",
        "wiki": "http://www.aabcframework.com/wiki/",
        "irc": "irc://irc.freenode.net/aabc",
        "source": "https://github.com/aabcsoft/aabc2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "aabcsoft/yii2-jui": "~2.0.0",
        "aabcsoft/aabc2": "~2.0.6",
        "aabcsoft/aabc2-bootstrap": "~2.0.0",
        "aabcsoft/aabc2-swiftmailer": "~2.0.0"
    },
    "require-dev": {
        "aabcsoft/aabc2-debug": "~2.0.0",
        "aabcsoft/aabc2-gii": "~2.0.0",
        "aabcsoft/aabc2-faker": "~2.0.0",

        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    }
}
